{
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "tsx": false,
        "decorators": true
      },
      "target": "es2017",
      "keepClassNames": true,
      "transform": {
        "legacyDecorator": true,
        "decoratorMetadata": true
      },
      "baseUrl": "src"
    },
    "module": {
      "type": "commonjs",
      "noInterop": true
    }
  }
